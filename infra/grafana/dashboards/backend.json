{
  "title": "Backend Overview",
  "panels": [
    { "type": "stat", "title": "HTTP 5xx (5m)", "targets": [{"expr": "sum(rate(http_server_requests_seconds_count{outcome=\"SERVER_ERROR\"}[5m]))"}] },
    { "type": "stat", "title": "HTTP 2xx (5m)", "targets": [{"expr": "sum(rate(http_server_requests_seconds_count{outcome=\"SUCCESS\"}[5m]))"}] },
    { "type": "graph", "title": "Req duration p95", "targets": [{"expr": "histogram_quantile(0.95, sum(rate(http_server_requests_seconds_bucket[5m])) by (le))"}] },
    { "type": "graph", "title": "JVM Heap Used", "targets": [{"expr": "jvm_memory_used_bytes{area=\"heap\"}"}] },
    { "type": "graph", "title": "GC Pause", "targets": [{"expr": "sum(rate(jvm_gc_pause_seconds_sum[5m]))"}] }
  ]
}






